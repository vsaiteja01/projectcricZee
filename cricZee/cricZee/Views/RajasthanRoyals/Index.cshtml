@model IEnumerable<cricZee.Models.RajasthanRoyal>

@{
    ViewData["Title"] = "Index";
}

<h1>Rajasthan Royals</h1>
<img src="~/lib/Rajasthanroyals1.jpg" />
<img src="~/lib/Rajasthanroyals2.jpg" />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PlayerId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlayerRole)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlayerRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            
        </tr>
}
    </tbody>
</table>
